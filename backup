  <script src="https://cdn.jsdelivr.net/npm/gsap@3.12.5/dist/gsap.min.js"></script>
  <script>
    let currentSlide = 0;
    const slides = document.querySelectorAll('.slide');
    const dots = document.querySelectorAll('.dot');
    const totalSlides = slides.length;

    function updateProgressIndicator() {
      dots.forEach((dot, index) => {
        dot.classList.toggle('active', index === currentSlide);
      });
    }

    function showSlide(index) {
      // Hide all slides
      slides.forEach(s => s.classList.remove('active'));
      
      // Show current slide
      slides[index].classList.add('active');
      
      // Update progress indicator
      updateProgressIndicator();

      // Animate slide content in
      const currentSlideEl = slides[index];
      const elements = currentSlideEl.children;
      
      // Reset elements
      gsap.set(elements, { y: 50, opacity: 0 });
      
      // Animate in with stagger
      gsap.to(elements, { 
        y: 0, 
        opacity: 1, 
        duration: 0.8, 
        stagger: 0.15,
        ease: "back.out(1.7)"
      });

      // Special animations for specific slides
      if (index === 0) {
        // Intro slide - dramatic entrance
        gsap.fromTo(currentSlideEl.querySelector('.intro-title'), 
          { scale: 0.5, rotation: -10 }, 
          { scale: 1, rotation: 0, duration: 1.2, ease: "elastic.out(1, 0.5)" });
      }
      
      if (index === 11) {
        // Trophy case - animate cards
        gsap.fromTo(currentSlideEl.querySelectorAll('.achievement-card'), 
          { scale: 0, rotation: 180 }, 
          { scale: 1, rotation: 0, duration: 0.8, stagger: 0.1, ease: "back.out(1.7)", delay: 0.3 });
      }

      // Animate big numbers with counting effect
      const bigNumber = currentSlideEl.querySelector('.big-number');
      if (bigNumber && index > 0 && index < 11) {
        const finalValue = bigNumber.textContent;
        const numValue = parseFloat(finalValue.replace(/[^0-9.]/g, ''));
        if (!isNaN(numValue)) {
          gsap.fromTo({ value: 0 }, { 
            value: numValue, 
            duration: 1.5, 
            ease: "power2.out",
            delay: 0.3,
            onUpdate: function() {
              if (finalValue.includes('%')) {
                bigNumber.textContent = Math.round(this.targets()[0].value) + '%';
              } else {
                bigNumber.textContent = Math.round(this.targets()[0].value).toLocaleString();
              }
            },
            onComplete: function() {
              bigNumber.textContent = finalValue;
            }
          });
        }
      }
    }

    function nextSlide() {
      currentSlide = (currentSlide + 1) % totalSlides;
      showSlide(currentSlide);
    }

    function prevSlide() {
      currentSlide = (currentSlide - 1 + totalSlides) % totalSlides;
      showSlide(currentSlide);
    }

    // Initialize
    showSlide(0);

    // Auto-advance every 4.5 seconds
    const autoAdvance = setInterval(nextSlide, 4500);

    // Manual navigation
    document.addEventListener('click', (e) => {
      if (e.target.classList.contains('dot')) {
        clearInterval(autoAdvance);
        currentSlide = parseInt(e.target.getAttribute('data-slide'));
        showSlide(currentSlide);
      } else {
        clearInterval(autoAdvance);
        nextSlide();
      }
    });

    document.addEventListener('keydown', (e) => {
      clearInterval(autoAdvance);
      if (e.key === 'ArrowRight' || e.key === ' ') {
        nextSlide();
      } else if (e.key === 'ArrowLeft') {
        prevSlide();
      }
    });

    // Pause auto-advance on hover
    document.addEventListener('mouseenter', () => clearInterval(autoAdvance));
  </script>

</body></html>